<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <settings>
        <setting name="mapUnderscoreToCamelCase" value="true" />
    </settings>

    <typeAliases>
        <typeAlias type="java.lang.String" alias="str" />
        <typeAlias type="java.lang.Integer" alias="int" />

        <typeAlias type="com.application.smartEdu.dto.MemberVO" alias="member" />
        <typeAlias type="com.application.smartEdu.dto.InstructorVO" alias="instructor" />
        <typeAlias type="com.application.smartEdu.dto.QuestionVO" alias="question" />
        <typeAlias type="com.application.smartEdu.dto.CommentVO" alias="comment" />
        <typeAlias type="com.application.smartEdu.dto.CourseVO" alias="course" />
        <typeAlias type="com.application.smartEdu.dto.ReviewVO" alias="review" />
    </typeAliases>

</configuration>